# fill with random passwords
OPERATOR_TOKEN=your-super-secret-operator-token

JWT_SECRET=your-super-secret-jwt-token-with-at-least-32-characters-long

POSTGRES_PASSWORD=your-super-secret-and-long-postgres-password

# some SMTP server to send your auth-mails with
SMTP_HOST=smtp.mailgun.org
SMTP_PORT=25
SMTP_USER=
SMTP_PASS=
SMTP_ADMIN_EMAIL=

# storage config
# file | s3
STORAGE_BACKEND=file
# name of s3 bucket where you want to store objects
STORAGE_S3_BUCKET=supa-storage-testing
# region where your bucket is located
STORAGE_REGION=us-east-1
# max file size (in bytes)
FILE_SIZE_LIMIT=52428800

# predefined; don't change these unless you know what you're doing
POSTGRES_PORT=5432
AUTH_PORT=9999
REST_PORT=3000
REALTIME_PORT=4000
KONG_PORT=8000
KONG_PORT_TLS=8443
